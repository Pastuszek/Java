        <resource-bundle>
            <base-name>productsMsg</base-name>
            <var>productsMsg</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>productCardMsg</base-name>
            <var>productCardMsg</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>productDetailsMsg</base-name>
            <var>productDetailsMsg</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>orderUserMsg</base-name>
            <var>orderUserMsg</var>
